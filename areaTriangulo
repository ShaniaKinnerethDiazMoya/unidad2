#include <stdio.h>

int main() {
    float base, altura;
    printf("Ingrese la base del tri치ngulo: ");
    scanf("%f", &base);
    printf("Ingrese la altura del tri치ngulo: ");
    scanf("%f", &altura);
    
    float area = (base * altura) / 2;
    printf("El 치rea del tri치ngulo es: %.2f\n", area);
    
    return 0;
}
