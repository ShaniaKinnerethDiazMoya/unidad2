#include <stdio.h>

int main() {
    int filas = 8;
    int columnas = 8;
    for (int i = 0; i < filas; i++) {
        for (int j = 0; j < columnas; j++) {
            if ((i + j) % 2 == 0) {
                printf("* ");
            } else {
                printf("  ");
            }
        }
        printf("\n");
    }
    return 0;
}
